pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  stages {
    stage('Test') {
      steps {
        sh 'node --version'
      }
    }
        stage('dev') {
      steps {
        sh 'java -version'
      }
    }
        stage('deploy') {
      steps {
        sh 'docker --version'
      }
    }
  }
}
